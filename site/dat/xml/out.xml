<url title="JMeter Plugins" template="plugins.tpl" 
     custom_logo="/img/site/logo.png"
     keywords="jmeter, plugins, load, performance, analysis, graphs, hadoop, webdriver, selenium"
     description="A custom set of plugins for Apache JMeter, not affiliated with Apache Software Foundation, graphs, load shapers, new functions."    
    
     class="PWE\Modules\HTMLPage\HTMLPage" src="index.html">
  
    <url link="home" title="Home" menu="1" menu_icon="/img/icons/home.gif" 
      class="PWE\Modules\RedirectAccepted\RedirectAccepted" base="/#"/>
    <url link="downloads" title="Downloads" menu="1" menu_icon="/img/icons/download-icon.png" 
         class="PWE\Modules\SimpleWiki\SimpleWiki" wiki_dir="dl" start_page="all"
         files_base="files" download_link="../file" accept="1">
        <url link="file" files_base="files" class="PWE\Modules\FileDownloads\FileDownloads" accept="2" />
    </url>
    <url link="wiki" class="PWE\Modules\SimpleWiki\SimpleWiki" wiki_dir="wiki" start_page="Start"
         accept="1" menu="1" menu_icon="/img/icons/icon_book.gif" title="Documentation"/>
    <!-- for local debugging
    <url link="wiki-local" class="PWE\Modules\SimpleWiki\SimpleWiki" wiki_dir="wiki" start_page="Start"
         accept="1" menu="0" menu_icon="/img/icons/icon_book.gif" title="Documentation"/>
    -->

    <url link="support" menu="1" menu_icon="/img/icons/icon-support.png" title="Support Forums" class="PWE\Modules\RedirectAccepted\RedirectAccepted" base="http://groups.google.com/forum/#!forum/jmeter-plugins" accept="1" />
    <url link="sources" title="Source Code" menu="1" menu_icon="/img/icons/github.png" class="PWE\Modules\RedirectAccepted\RedirectAccepted" base="https://github.com/undera/jmeter-plugins" />
    
    <url link="donations" title="Support the Project" menu="0" class="PWE\Modules\HTMLPage\HTMLPage" src="donate.html"/>
</url>